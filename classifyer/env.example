# AI Service Configuration
# Get your API keys from the respective service providers

# Google Vision API
# Get from: https://console.cloud.google.com/apis/credentials
EXPO_PUBLIC_GOOGLE_VISION_API_KEY=your-google-vision-api-key-here

# AWS Rekognition
# Get from: https://aws.amazon.com/rekognition/
EXPO_PUBLIC_AWS_ACCESS_KEY_ID=your-aws-access-key-id-here
EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key-here
EXPO_PUBLIC_AWS_REGION=us-east-1

# App Configuration
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_BUILD_NUMBER=1

# Optional: Custom Model URLs
EXPO_PUBLIC_CUSTOM_MODEL_URL=https://your-model-url.com/waste-classifier-model.json

# Optional: Analytics
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_CRASH_REPORTING_ENABLED=true
